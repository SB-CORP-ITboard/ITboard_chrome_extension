{
  "manifest_version": 3,
  "name": "ITboard Chrome Extension",
  "version": "0.0.1",
  "description": "ITboard Chrome Extension",
  "author": "jimpei",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "history",
    "background",
    "identity",
    "identity.email",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "src/js/background.js",
    "type": "module"
  },
  "host_permissions": [
    "http://localhost:3000/"
  ]
}